package com.shinhan.day07.CH8;

public interface ActionExample {
	public void work();
}
public class ActionExample {
	public static void main(String[] args) {
		ActionExample action =  new ActionExample() {
			public void work() {
				System.out.println("익명구현객체");
			}
		};
		ActionExample action2 =() -> System.out.println("Lamda표현식");
	}
}
