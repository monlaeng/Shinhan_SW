package com.shinhan.day05;

public class PlayBird {

	public static void main(String[] args) {
		Duck du1 = new Duck("꽥꽥이", 2, 15);
		Duck du2 = new Duck("꽉꽉이", 2, 18);
		Sparrow sp1 = new Sparrow("쨱짹이", 2, 10);
		Sparrow sp2 = new Sparrow("뾰롱이", 2, 8);
		
		du1.fly();
		du1.sing();
		du1.toString();
		
		du2.fly();
		du2.sing();
		du2.toString();
		
		sp1.fly();
		sp1.sing();
		sp1.toString();
		
		sp2.fly();
		sp2.sing();
		sp2.toString();
	}

}
