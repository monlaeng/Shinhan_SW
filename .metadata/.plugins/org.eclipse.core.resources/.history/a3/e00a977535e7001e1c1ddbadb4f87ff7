package com.shinhan.day08;

public class ExceptionTest2 {
	public static void main(String[] args) {
		method1(70);
	}

	private static void method1(int score) {

		//조건에 맞지않으면 강제로 예외를 발생시키고자 한다.
		if(score <= 70) throw new RuntimeException();
		System.out.println("--------------------");
	}
}
