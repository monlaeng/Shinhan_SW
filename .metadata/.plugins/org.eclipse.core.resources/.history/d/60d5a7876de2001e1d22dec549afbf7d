package com.shinhan.day04;

public class BookTest {

	public static void main(String[] args) {
		method1();
	}

	private static void method1() {
		//1.객체참조변수선언
		Book b1,b2;
		//2.객체생성
		b1 = new Book();
		b2 = new Book();
		//3.객체사용
		System.out.println(b1.title);
		System.out.println(b2.title);
	}

}
