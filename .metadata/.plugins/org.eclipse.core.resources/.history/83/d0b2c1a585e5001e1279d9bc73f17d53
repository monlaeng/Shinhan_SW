package com.shinhan.day05;

public class SchoolTest {

	public static void main(String[] args) {
		Student st1 = new Student("홍길동", 20, 200201);
		Teacher te1 = new Teacher("이순신", 30, "JAVA");
		Employee em1 = new Employee("유관순", 40, "교무과");
		
		st1.print();
		te1.print();
		em1.print();
		
		st1.setAge(23);
		te1.setName("장영실");
		em1.setDept("행정");
		
		st1.print();
		te1.print();
		em1.print();
	}

}
